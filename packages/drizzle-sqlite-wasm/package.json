{
	"name": "@firtoz/drizzle-sqlite-wasm",
	"version": "0.0.0",
	"description": "Drizzle SQLite WASM bindings",
	"main": "./src/index.ts",
	"module": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts",
			"require": "./src/index.ts"
		}
	},
	"files": [
		"src/**/*.ts",
		"!src/**/*.test.ts",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"typecheck": "tsc --noEmit -p ./tsconfig.json",
		"lint": "biome check --write src",
		"lint:ci": "biome ci src",
		"format": "biome format src --write",
		"test": "bun test --pass-with-no-tests",
		"test:watch": "bun test --watch"
	},
	"keywords": [
		"typescript",
		"sqlite",
		"wasm",
		"drizzle"
	],
	"author": "Firtina Ozbalikchi <firtoz@github.com>",
	"license": "MIT",
	"homepage": "https://github.com/firtoz/fullstack-toolkit#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/firtoz/fullstack-toolkit.git",
		"directory": "packages/drizzle-sqlite-wasm"
	},
	"bugs": {
		"url": "https://github.com/firtoz/fullstack-toolkit/issues"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@sqlite.org/sqlite-wasm": "^3.50.4-build1",
		"drizzle-orm": "^0.44.7"
	},
	"devDependencies": {}
}
